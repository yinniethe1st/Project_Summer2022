using System;

namespace MyApplication
{
  class Program
  {
    static void Main(string[] args)
    {
      int x = 16;
      int y = 18;
      int z = 17;
      if (x > y && x > z)
      {
        Console.WriteLine("x is the greatest number");
      } else if ( x > z && x < y ) {
        Console.WriteLine("x is the not greatest number");

      } else {
        Console.WriteLine("x is the smallest number");

      }
    }
  }
}